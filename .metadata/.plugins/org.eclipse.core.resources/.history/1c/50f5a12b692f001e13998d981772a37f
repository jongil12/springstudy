<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="members">
	<select id="list" resultType="mvo">
		select *from members
	</select>
	<insert id="insert" parameterType="mvo">
		insert into members()
		values()
	</insert>
</mapper>


